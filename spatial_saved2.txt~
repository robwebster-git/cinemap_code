DECLARE
  dist NUMBER;
  geoma SDO_GEOMETRY;
  geomb SDO_GEOMETRY;

BEGIN
SELECT a.ora_geometry FROM busroutes_8307 a;b.geom FROM s1434165.cinemas b;

SDO_GEOM.SDO_CLOSEST_POINTS(busroutes_8307.ora_geometry, s1434165.cinemas.geom, 0.005, NULL, dist, geoma, geomb);

DBMS_OUTPUT.PUT_LINE('dist output parameter value = ' || dist);
END;
/
